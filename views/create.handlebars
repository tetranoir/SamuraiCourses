<!doctype html>

<html>
<head>
	{{> searchExperiment}}
	<title>Create Schedules</title>	
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	{{> styles}}
	<link href="../css/homepage.css" rel="stylesheet">
	<link href="../css/search.css" rel="stylesheet">
	<link href="../css/awesomplete.css" rel="stylesheet">
	<link rel="icon" type="../image/png" href="favicon.png">
	<link rel="shortcut icon" type="../image/png" href="favicon.png">

	<!-- this is a comment in HTML -->

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->        
</head>

<body>
	{{> navbar}}
	<div class="container">
		<div class="jumbotron">
			<h1>Create Schedule</h1>
			<p>Hopefully one of these is useful</p>
		</div>
		<div class="row">
			<div class="col-md-offset-3 col-md-6 col-xs-12 schedule">
				<div class="row schedule-header no-gutter">
					<div class="col-xs-1">&nbsp</div>
					<div class="col-xs-2 schedule-day left-end">Mon<text class="hidden-xs">day</text></div>
					<div class="col-xs-2 schedule-day">Tues<text class="hidden-xs">day</text></div>
					<div class="col-xs-2 schedule-day">Wed<text class="hidden-xs">nesday</text></div>
					<div class="col-xs-2 schedule-day">Thur<text class="hidden-xs">sday</text></div>
					<div class="col-xs-2 schedule-day">Fri<text class="hidden-xs">day</text></div>
				</div>
				<!--WHERE THE MINIMAP GENERATION GOES-->
				<div class="row no-gutter">
					<div class="col-xs-offset-1 col-xs-10 minimap">
						
					</div>
				</div>
			</div>
		</div>
		<div class="row">
			<!-- Errors -->
			<div class="alert alert-danger" id="no-schedule-error" style="display: none;">
				Adding that class generates no possible schedules!
			</div>
			<div class="alert alert-info" id="already-added-error" style="display: none;">
				That class has already been added!
			</div>
			<div class="alert alert-warning" id="not-found-error" style="display: none;">
				Class not found!
			</div>
			<div class="alert alert-success" id="add-sucess" style="display: none;">
			</div>
			
			
			<!-- search bar -->
			<div class="container">	
				<div class="col-xs-8 col-md-2"> 
					<div class="form-group">
						<input type="text" class="form-control" id="search-complete" placeholder="ex: CSE120">
					</div>
				</div>
				<div class="">
					<button type="button" href="#search" class="btn btn-default">add class</button>
				</div>			
			</div>

		</div>
		<div class="row schedule-pages">
		  <ul class="nav nav-tabs pull-right">
		    <li class="schedule-pg active"><a data-toggle="tab" href="1">1</a></li>
		    <li class="schedule-pg"><a data-toggle="tab" href="2">2</a></li>
		    <li class="schedule-pg"><a data-toggle="tab" href="3">You have javascript disabled lol</a></li>
		  </ul>
		</div>
		<br>
		<div class="row daily container">
			<ul class="nav nav-pills nav-justified nav-color">
				<li class="active daily-day"><a data-toggle="pill" href="#monday">M</a></li>
				<li class="daily-day"><a data-toggle="pill" href="#tuesday">T</a></li>
				<li class="daily-day"><a data-toggle="pill" href="#wednesday">W</a></li>
				<li class="daily-day"><a data-toggle="pill" href="#thursday">T</a></li>
				<li class="daily-day"><a data-toggle="pill" href="#friday">F</a></li>
			</ul>
			<hr class="low-margin"/>
			<div class="daily-timecells">
				<!--WHERE THE CALEDAR GENERATION GOES-->
				<div class="tab-content">
					<div id="monday" class="tab-pane fade in active">
						<div class="row"></div>
					</div>
					<div id="tuesday" class="tab-pane fade">
						<div class="row"></div>
					</div>
					<div id="wednesday" class="tab-pane fade">
						<div class="row"></div>
					</div>
					<div id="thursday" class="tab-pane fade">
						<div class="row"></div>
					</div>
					<div id="friday" class="tab-pane fade">
						<div class="row"></div>
					</div>
				</div>

				<!-- Modal-->
				<div id="class-info" class="modal fade">
				  <div class="modal-dialog">
				    <!-- Modal content-->
				    <div class="modal-content">
				      <div class="modal-header">
				        <button type="button" class="close" data-dismiss="modal">&times;</button>
				        <h4 class="modal-title">Class 1</h4>
				      </div>
				      <div class="modal-body" style="white-space: pre-line">
				        <p>This is a class where you have to memorize and learn.</p>
				      </div>
				      <div class="modal-footer">
				      	<p>Professor Staff</p>
				      	<button type="button" class="btn btn-danger pull-left" data-dismiss="modal" id="remove-class">Remove Class</button>
				        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
				      </div>
				    </div>
				  </div>
				</div>
			</div>
		</div>

	</div>
	<!-- Search Form -->
	<!-- 
	<div id="search"> 
		<span class="close">X</span>
		<form role="search" id="searchform" action="/search" method="get">
			<input id="search-complete" value="" name="q" type="search" placeholder="Search with [Subject] [Num]"/>
			
		</form>
	</div>
	-->

	<script src="https://code.jquery.com/jquery.js"></script>
	<script src="../js/bootstrap.js"></script>
	<!-- awesomplete from web search -->
	<script src="../js/awesomplete.js"></script>
	<script src="../js/search.js"></script>
	<script src="../js/create2.js"></script>
	{{> googleAnalytics}}
</body>
</html>
